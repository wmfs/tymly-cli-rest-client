  },
  "homepage": "https://github.com/wmfs/tymly-cli#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "auth0-js": "^9.6.0",
    "boom": "^7.2.0",
    "debug": "^3.1.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "codecov": "3.0.2",
    "cz-conventional-changelog": "2.1.0",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "semantic-release": "^15.5.0",
    "standard": "11.0.1",
